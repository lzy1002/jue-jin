.home-wrapper
  width 100%
  height 100%
  overflow hidden
  padding-bottom 40px

.move-enter
  transform translateX(100%)

.move-enter-active
  transition all 1s ease
  transform translateX(0)

.move-exit
  transition all 1s ease
  transform translateX(0)

.move-exit-active
  transform translateX(-100%)
