@import "../../../assets/stylus/variable.styl"

.loginTip-wrapper
  width 100%
  height 200px
  display flex
  flex-direction column
  justify-content center
  align-items center
  background-color $color-background-d
  .iconfont
    font-size 54px
    color #e7e9e9
    margin-bottom 10px
  .tip
    font-size $font-size-medium-m
    color $color-text-l
    margin-bottom 15px
  .login-btn
    font-size $font-size-medium-m
    color #fff
    padding 9px 15px
    background-color $color-theme
    border-radius 2px
